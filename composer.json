{
    "name": "velocity-sports-labs/data-center-laravel-sdk",
    "description": "VSL Data Center is a bridge for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "vsl",
        "velocity sports labs",
        "data center"
    ],
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "VelocitySportsLabs\\DataCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VelocitySportsLabs\\DataCenter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "VelocitySportsLabs\\DataCenter\\DataCenterServiceProvider"
            ],
            "aliases": {
                "DataCenter": "VelocitySportsLabs\\DataCenter\\DataCenter"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "velocity-sports-labs/datacenter-php-sdk": "^0.1",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.6",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.18|^9.0",
        "pestphp/pest": "^2.30",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
